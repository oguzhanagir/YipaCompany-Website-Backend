@model Yipa.Entities.Concrete.Service

@{
    ViewData["Title"] = "AddService";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<form method="post" class="form-group">

    @Html.Label("Service Başlığı")
    @Html.TextBoxFor(x=>x.Title, new { @class = "form-control input-default" })
    <br />
     @Html.Label("Service İçeriği")
    @Html.TextBoxFor(x=>x.Content, new { @class = "form-control input-default" })
    <br />

    <button class="btn btn-group btn-success" type="submit" >Kaydet</button>
</form>