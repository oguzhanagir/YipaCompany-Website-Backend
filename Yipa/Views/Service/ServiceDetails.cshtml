@model IEnumerable<Yipa.Entities.Concrete.Service>
@{
    ViewData["Title"] = "ServiceDetails";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


@foreach (var item in Model)
{
    <div class="dark-background-content text-center" style="padding: 150px 80px 50px 80px;">
    <h1 class="text-light mb-4" style="font-weight:700">@item.Title</h1>
@*    <p class="header-text mt-5">Agency provides a full service range including technical skills, design, business understanding.</p>
*@</div>


<div class="container pt-2 pb-5 pl-5 pr-5" style=" margin-bottom:50px;">
    <div class="row">
        <div class="col-lg-12 col-sm-12 ">
            <p class="text p-5" style="font-weight:600">
               @Html.Raw(@item.Content)
            </p>
        </div>
    </div>
</div>

}


@Html.Partial("~/Views/Newsletter/MainNewsletter.cshtml")
@Html.Partial("~/Views/Home/Footer.cshtml")